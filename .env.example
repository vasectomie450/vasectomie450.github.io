# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Application Environment
NODE_ENV=production
VITE_APP_ENV=production

# API Configuration
VITE_API_BASE_URL=https://your-api-domain.com
VITE_APP_VERSION=1.0.0

# Google Services (if needed)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_GOOGLE_ANALYTICS_ID=your_google_analytics_id_here

# Contact Form Configuration
VITE_CONTACT_EMAIL=info@vasectomie450.com
VITE_CONTACT_PHONE=+14509992973

# Security Configuration
VITE_ENABLE_SECURITY_HEADERS=true
VITE_CSP_REPORT_URI=https://your-domain.com/csp-report

# Rate Limiting
VITE_RATE_LIMIT_MAX_REQUESTS=100
VITE_RATE_LIMIT_WINDOW_MS=900000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT=true
VITE_ENABLE_REVIEWS=true

# Social Media Links
VITE_FACEBOOK_URL=https://facebook.com/vasectomie450
VITE_TWITTER_URL=https://twitter.com/vasectomie450
VITE_LINKEDIN_URL=https://linkedin.com/company/vasectomie450
